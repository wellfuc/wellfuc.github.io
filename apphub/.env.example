APP_ENV=production
APP_NAME=AppHub
APP_URL=https://example.com
APPHUB_ROOT=/apphub
SECRET_KEY=change-me-please
DATABASE_URL=postgresql://apphub:apphub@127.0.0.1:5432/apphub
STORAGE_ROOT=/var/www/tools/apphub/storage
MAX_UPLOAD_MB=500
ALLOWED_UPLOAD_EXTENSIONS=.dmg,.exe,.msi,.pkg,.zip,.tar.gz,.tgz
ALLOWED_MEDIA_EXTENSIONS=.png,.jpg,.jpeg,.webp,.gif,.svg
CLAMAV_ENABLED=false
CLAMAV_SOCKET=/var/run/clamav/clamd.ctl
